{
  "scopeName": "source.oyster",
  "name": "Oyster 4S",
  "patterns": [
    {
      "name": "entity.name.function.oyster",
      "match": "^([A-Za-z_][A-Za-z0-9_]*)\\s*\\["
    },
    {
      "name": "comment.line.number-sign.oyster",
      "match": "^(#.*)$"
    },
    {
      "name": "string.quoted.double.oyster",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.oyster",
          "match": "\\."
        }
      ]
    },
    {
      "name": "constant.numeric.integer.oyster",
      "match": "-?\\b\\d+\\b"
    },
    {
      "name": "constant.language.boolean.oyster",
      "match": "\\b(True|False)\\b"
    }
  ],
  "repository": {},
  "fileTypes": ["txt"],
  "uuid": "b1e1e1e1-1111-1111-1111-111111111111"
}
